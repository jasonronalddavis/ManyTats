



<h2> <%= @user.name %> </h2><br><br>


<%if session[:user_id] %>




<h1> Your Artists: </h1><br>
<% @artists.all.each do |artist| %>
<h2> <%=link_to artist.name, artist_path(artist)%><br></h2>
<%end%>
<% if @artists.count == 0 %>
<h1> You have none! </h1>  
<%end%><br><br>





<%elsif session[:artist_id] %>
<h1>All Artists </h1>

<% @artists.all.each do |artist|%>
<h1><%= link_to artist.name, artist_path(artist) %></h1> <br><br>
<%end%>


<%else%>
<%= link_to "Sign Up as User", new_admin_user_path %><br><br>
<%= link_to "Sign Up as Artist", new_admin_artist_path %><br><br>
<%= link_to "Sign in", admin_signin_path, method: 'get' %><br><br>


<%end%>


<%= link_to "Return home", admin_root_path %><br>



<%= link_to "Log Out", signout_path, method: :delete %><br><br>