<%= form_for [:admin,@artist] do |f| %>
    <%= f.label :name %><br>
    <%= f.text_field :name, class: "NAME" %>
    <br>
    <%= f.label :bio %><br>
    bio:<%= f.text_area :Short Description %>
    <%= f.label :password %>
    <%= f.text_field :password %>
    <%= f.submit "Create Artist" %> 
<% end %> 
